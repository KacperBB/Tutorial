{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 71,
      "minLength": 3
    },
    "description": {
      "type": "text",
      "required": false,
      "minLength": 3
    },
    "email": {
      "type": "email",
      "required": false
    },
    "orderid": {
      "type": "uid",
      "required": false,
      "private": true
    },
    "paymentinfo": {
      "type": "json",
      "required": false
    },
    "products": {
      "type": "json",
      "required": false
    },
    "adress": {
      "type": "text",
      "required": false
    },
    "transactionid": {
      "type": "uid"
    },
    "avaliableQty": {
      "type": "integer",
      "required": true
    },
    "satus": {
      "type": "string",
      "required": false
    },
    "color": {
      "type": "string"
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "slug": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "rice": {
      "type": "richtext"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "rice",
        "mix",
        "test",
        "lol"
      ]
    }
  }
}
